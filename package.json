{
  "name": "@upfluence/oss-components",
  "version": "3.31.27",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/upfluence/oss-components",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test --silent -r dot",
    "test:ember-compatibility": "ember try:each",
    "storybook": "cp -R public/assets/fonts app/styles/core && start-storybook -p 6006 -s dist",
    "build-storybook": "build-storybook -s dist",
    "prepack": "ember ts:precompile",
    "install-pre-push-hook": "cp ./scripts/pre-push ./.git/hooks/pre-push",
    "remove-pre-push-hook": "rm -rf ./.git/hooks/pre-push"
  },
  "dependencies": {
