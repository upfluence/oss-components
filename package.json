{
  "name": "@upfluence/oss-components",
  "version": "3.53.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/upfluence/oss-components",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test --silent -r dot",
    "test:ember-compatibility": "ember try:each",
    "storybook": "cp -R public/assets/fonts app/styles/core && start-storybook -p 6006 -s dist",
    "build-storybook": "build-storybook -s dist",
    "prepack": "ember ts:precompile",
    "install-pre-push-hook": "cp ./scripts/pre-push ./.git/hooks/pre-push",
    "remove-pre-push-hook": "rm -rf ./.git/hooks/pre-push",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/macros": "^1.13.4",
    "@fortawesome/fontawesome-pro": "git+ssh://git@github.com/upfluence/fontawesome-pro.git",
    "babel-plugin-debug-macros": "^0.3.1",
    "bootstrap": "^3.3.7",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "calculate-cache-key-for-tree": "^2.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-ifa": "^0.10.0",
    "ember-cli-less": "^2.0.6",
    "ember-cli-typescript": "^5.2.1",
    "ember-intl": "^4.1.0",
    "ember-named-blocks-polyfill": "^0.2.4",
    "ember-truth-helpers": "^3.1.1",
    "ion-rangeslider": "^2.3.1",
    "money-formatter": "^0.1.4"
  },
  "peerDependencies": {
    "qunit": "^2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@ember/jquery": "^2.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.1",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@glint/template": "^1.2.1",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/ember": "6.4.9",
    "@storybook/ember-cli-storybook": "^0.4.0",
    "@tsconfig/ember": "^3.0.3",
    "@types/ember": "^4.0.10",
    "@types/ember-qunit": "^6.1.1",
    "@types/ember-resolver": "^9.0.0",
    "@types/ember__application": "^4.0.10",
    "@types/ember__array": "^4.0.9",
    "@types/ember__component": "^4.0.21",
    "@types/ember__controller": "^4.0.11",
    "@types/ember__debug": "^4.0.7",
    "@types/ember__destroyable": "^4.0.4",
    "@types/ember__engine": "^4.0.10",
    "@types/ember__error": "^4.0.5",
    "@types/ember__object": "^4.0.11",
    "@types/ember__polyfills": "^4.0.5",
    "@types/ember__routing": "^4.0.19",
    "@types/ember__runloop": "^4.0.8",
    "@types/ember__service": "^4.0.8",
    "@types/ember__string": "^3.0.13",
    "@types/ember__template": "^4.0.5",
    "@types/ember__test": "^4.0.5",
    "@types/ember__utils": "^4.0.6",
    "@types/jquery": "^3.5.29",
    "@types/qunit": "^2.19.9",
    "@types/rsvp": "^4.0.8",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.7",
    "ember-cli": "~3.28.6",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-typescript-blueprints": "upfluence/ember-cli-typescript-blueprints#ember-4-update",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-qunit": "6.1.1",
    "ember-resolver": "8.0.3",
    "ember-sinon-qunit": "^5.0.0",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "less-loader": "^7.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "style-loader": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "5"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
